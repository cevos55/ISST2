{
  "version": 2,
  "builds": [
    {
      "src": "api/server.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/api/(.*)",
      "dest": "api/server.mjs"
    },
    {
      "src": "/(.*)",
      "dest": "api/server.mjs"
    }
  ]
}
